<BorderPane maxHeight="928" maxWidth="800" minHeight="928" minWidth="800" prefHeight="928" prefWidth="800" xmlns="http://javafx.com/javafx/8" xmlns:fx="http://javafx.com/fxml/1" fx:controller="application.UserController">
 <stylesheets>
    <URL value="@UserGuiStyle.css" />
  </stylesheets>
	<top>
		<HBox prefHeight="24.0" prefWidth="715.0" BorderPane.alignment="CENTER">
			<children>
				<MenuBar HBox.hgrow="ALWAYS">
					<menus>
						<Menu mnemonicParsing="false" text="File">
							<items>
                        <MenuItem mnemonicParsing="false" onAction="#onFileOpen" text="Open File" />                        
								<MenuItem mnemonicParsing="false" onAction="#onClose" text="Close" />
							</items>
						</Menu>
					</menus>
				</MenuBar>
			</children>
		</HBox>
	</top>
	<bottom>
		<HBox prefHeight="63.0" prefWidth="716.0" BorderPane.alignment="CENTER_RIGHT">
			<children>
            <Group>
               <children>
                  <AnchorPane prefHeight="66.0" prefWidth="716.0">
                     <children>
                  				<Button fx:id="backButton" graphicTextGap="5.0" layoutX="14.0" layoutY="14.0" mnemonicParsing="false" onAction="#onBack" prefHeight="40.0" prefWidth="70.0" text="Back" textAlignment="CENTER">
                  				
                                       <font>
                              <Font name="Times New Roman Bold" size="13.0" />
                           </font>
                           <opaqueInsets>
                              <Insets />
                           </opaqueInsets>
                  				</Button>
                        <Button fx:id="finishButton" layoutX="324.0" layoutY="9.0" mnemonicParsing="false" onAction="#onFinnish" prefHeight="50.0" prefWidth="68.0" text="Finish">
                           <font>
                              <Font name="Times New Roman Bold" size="15.0" />
                           </font>
                        </Button>
            				<Button fx:id="nextButton" contentDisplay="RIGHT" layoutX="632.0" layoutY="14.0" mnemonicParsing="false" onAction="#onNext" prefHeight="40.0" prefWidth="70.0" text="Next" textAlignment="CENTER">
                           <font>
                              <Font name="Times New Roman Bold" size="13.0" />
                           </font>
                           <padding>
                              <Insets right="5.0" />
                           </padding>
            				</Button>
                     </children>
                  </AnchorPane>
               </children></Group>
			</children>
		</HBox>
	</bottom>
   <center>
      <AnchorPane prefHeight="620.0" prefWidth="715.0" BorderPane.alignment="TOP_CENTER">
         <children>
            <TabPane fx:id="tabPane" prefHeight="846.0" prefWidth="800.0" tabClosingPolicy="UNAVAILABLE" />
         </children>
         <BorderPane.margin>
            <Insets />
         </BorderPane.margin>
      </AnchorPane>
   </center>

</BorderPane>
